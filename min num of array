#include <stdio.h>
int main()
{
    int a[10],n,i,min;
    printf("enter the size of an array:");
    scanf("%d",&n);
    printf("enter the elements into an array:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    //max of elements
    min=a[0];
    for(i=0;i<n;i++)
    {
        if(a[i]<min)
        {
            min=a[i];
        }
    }
    printf("the min value is:%d",min);
    return 0;
}
