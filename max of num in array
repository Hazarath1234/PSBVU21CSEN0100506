#include <stdio.h>
int main()
{
    int a[10],n,i,max;
    printf("enter the size of an array:");
    scanf("%d",&n);
    printf("enter the elements into an array:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    //max of elements
    max=a[0];
    for(i=0;i<n;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
    }
    printf("the max value is:%d",max);
    return 0;
}
