#include <stdio.h>
void sum();
void sub();
void mul();
void div();
void mod();
int main()
{
	 sub();
}
void sum()
{
	int a,b;
	printf("\n enter two numbers");
	scanf("%d %d",&a,&b);
	printf("the sum is %d",a+b);
}
void sub()
{
	int a,b;
	printf("\n enter the two numbers");
	scanf("%d %d",&a,&b);
	printf("the sub is %d",a-b);
}
void mul()
{
	int a,b;
	printf("\n enter the two numbers");
	scanf("%d %d",&a,&b);
	printf("the mul is %d",a*b);
}
void div()
{
	int a,b;
	printf("\n enter the two numbers");
	scanf("%d %d",&a,&b);
	printf("the div is %d",a/b);
}
void mod()
{
	int a,b;
	printf("\n enter the two numbers");
	scanf("%d %d",&a,&b);
	printf("the mod is %d",a%b);
}
