#include <stdio.h>
#include <math.h>
double power(double b, double e);
int main()
{
    double base,exp,d;
    printf("enter the base and exp");
    scanf("%lf%lf",&base,&exp);
    d=power(base,exp);
    printf("%lf",d);
}
double power(double b, double e)
{
    double result;
    result=pow(b,e);
    return result;
}
